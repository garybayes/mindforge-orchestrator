{
  "name": "Task-Assistant",
  "url": "https://github.com/automated-assistant-system",
  "hook_attributes": {
    "url": "https://your-server-domain.com/github/webhook"
  },
  "redirect_url": "https://github.com/apps/task-assistant/installations/new",
  "callback_urls": [],
  "public": false,
  "default_permissions": {
    "issues": "write",
    "metadata": "read",
    "contents": "read",
    "actions": "read",
    "pull_requests": "read"
  },
  "default_events": [
    "issues",
    "issue_comment",
    "pull_request",
    "workflow_run"
  ]
}
